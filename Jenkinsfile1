pipeline
{
stages{

    stage('build')
    {
        steps {
            
        
          echo "Hello"
           
        }

    }

    stage('test')
    { 
        parallel {
            stage('testA')
            {
               agent { label 'DevServer' }
                 steps{
                    echo " This is test A"
                    sh "mvn test"
                }
                
            }
            stage('testB')
            {
                agent { label 'DevServer' }
                steps{
                echo "this is test B"
                sh "mvn test"
                }
            }
        }
        

    }
   
}

}
